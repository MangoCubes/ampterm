{
  "keybindings": {
    "Common": {
	  "<Ctrl-w><Left>": "MoveLeft",
	  "<Ctrl-w><Right>": "MoveRight",
	  "<Ctrl-w><Up>": "MoveUp",
	  "<Ctrl-w><Down>": "MoveDown",
	  "<Shift-g>": {
		  "Local": "Bottom"
	  },
	  "<g><g>": {
		  "Local": "Top"
	  },
	  "<Shift-r>": {
		  "Local": "Refresh"
	  },
	  "<Up>": {
		  "Local": "Up"
	  },
	  "<Down>":{
		  "Local": "Down"
	  },
	  "<Left>":{
		  "Local": "Left"
	  },
	  "<Right>":{
		  "Local": "Right"
	  },
	  "<Enter>":{
		  "Local": "Confirm"
	  },
	  "<Q>":{
		  "Local": "Cancel"
	  },
	  "<Esc>":"EndKeySeq",
	  "<s>": "Play",
	  "<Shift-s>": "Pause"
    },
  },
  // Use legacy authentication that sends the password instead of token and salt
  // This may be necessary if you are using servers like Ampache, which does not support token-based authentication yet
  "use_legacy_auth": false,
  "auth": {
    "url_command": "",
    "username_command": "",
    "password_command": "",
  },
  "unsafe_auth": {
    "url": "",
    "username": "",
    "password": "",
  }
}
